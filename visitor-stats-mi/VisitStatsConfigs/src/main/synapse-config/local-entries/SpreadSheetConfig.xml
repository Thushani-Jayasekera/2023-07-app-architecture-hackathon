<?xml version="1.0" encoding="UTF-8"?>
<localEntry key="SpreadSheetConfig" xmlns="http://ws.apache.org/ns/synapse">
    <googlespreadsheet.init>
        <accessToken>{$system:spreadsheet.accessToken}</accessToken>
        <apiUrl>https://sheets.googleapis.com/v4/spreadsheets</apiUrl>
        <clientId>{$system:spreadsheet.clientId}</clientId>
        <clientSecret>{$system:spreadsheet.clientSecret}</clientSecret>
        <refreshToken>{$system:spreadsheet.refreshToken}</refreshToken>
    </googlespreadsheet.init>
</localEntry>
